{
  "name": "nestjs-vue3-ssr",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "pm2": "^5.3.1",
    "prom-client": "^15.1.0",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ssr-core": "^6.2.29",
    "ssr-core-vue3": "^6.0.0",
    "ssr-hoc-vue3": "^6.0.0",
    "ssr-server-utils": "^6.0.0",
    "tailwindcss": "^3.4.1",
    "vant": "^3.6.11",
    "video.js": "^8.9.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue3-count-to": "^1.1.2",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint-config-standard-vue-ts": "^1.0.5",
    "ssr": "^6.0.0",
    "ssr-plugin-nestjs": "^6.0.0",
    "ssr-plugin-vue3": "^6.0.0",
    "ssr-types": "^6.0.0",
    "typescript": "^4.0.0",
    "webpack": "^4.0.0"
  },
  "scripts": {
    "prod": "yarn && cross-env USE_ENV=production ssr build && pm2-runtime start pm2.config.js",
    "tech": "yarn && cross-env USE_ENV=tech ssr build && pm2-runtime start pm2dev.config.js",
    "stop": "pm2 stop pm2.config.js",
    "start": "cross-env USE_ENV=development VUE_APP_BASE_URL=https://h5.yowin.tech  VUE_APP_BASE_URL_V2=https://s.yowin.tech ssr start",
    "dev": "yarn start",
    "start:vite": "ssr start --vite",
    "build": "ssr build",
    "build:vite": "ssr build --vite",
    "deploy": "ssr build && ssr deploy",
    "lint": "eslint . --ext .js,.tsx,.ts,.vue --cache",
    "lint:fix": "eslint . --ext .js,.tsx,.ts,.vue --cache --fix",
    "oci": "ssr build && pm2 start pm2.config.js"
  }
}
